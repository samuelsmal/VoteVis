<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:votevis='urn:import:com.votevis.client.presenter'>

  <ui:style>
     .vis-header {
		text-align: center;
		font-size: 200%;
		position: center;
		margin: 0 auto;
	 }

	.vis-body {
	position: center;
	text_align: center;
    width: 1900px;
    height: 700px;
	margin: auto;
	}
  </ui:style>

  <g:HTMLPanel>
      <div>
        <header class='{style.vis-header}'><span ui:field='titleSpan'/></header>
        
        </div>
	
	
	<div class='{style.vis-body}' ui:field='bodyDiv'>
	</div>
	
	<div>
	<votevis:CommentPresenter ui:field='comment'/>
	</div>
	
  </g:HTMLPanel>

</ui:UiBinder>
